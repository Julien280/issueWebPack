{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@test/*": ["./test/*"],
      "@shared/*": ["./shared/*"],
      "@styles/*": ["src/styles/*"],
      
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./dist",
    "noImplicitAny":false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,//true,
    "jsx": "react-jsx"
  },
  "include": ["src","shared","test", "initDebugScripts.js"],
}
